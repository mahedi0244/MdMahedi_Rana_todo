<h1>To Do List:</h1>
<ul>	
	{{#each data}}
	<li><a href="/users/{{this.id}}">{{this.taskname}}</a></li>
	{{/each}}
</ul>

<form action = "/users" method="POST">
	<input type="text" placeholder = "taskname" name="taskname">
	<input type="text" placeholder = "description" name="description">
	<input type="submit">	
</form>